AppBundle\Entity\StyleDanse:
    type: entity
    table: styleDanse
    repositoryClass: AppBundle\Repository\StyleDanseRepository
    id:
        id:
            type: integer
            id: true
            generator:
                strategy: AUTO
    fields:
        nom:
            type: string
            length: 255
            unique: true

        description:
            type: text
            unique: false
            nullable: true

    manyToMany:
        typeDanses:
            targetEntity: AppBundle\Entity\TypeDanse
            mappedBy: styleDanses

    lifecycleCallbacks: {}


